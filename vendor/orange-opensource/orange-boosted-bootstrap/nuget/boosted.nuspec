<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>boosted</id>
    <version>4.5.3</version>
    <title>Boosted CSS</title>
    <authors>The Boosted Authors, Orange, The Bootstrap Authors, Twitter Inc.</authors>
    <owners>Orange</owners>
    <description>Fork of the most popular front-end framework for developing responsive, mobile first projects on the web.</description>
    <releaseNotes>https://github.com/Orange-OpenSource/Orange-Boosted-Bootstrap/releases</releaseNotes>
    <summary>Boosted framework in CSS. Includes fonts and JavaScript</summary>
    <language>en-us</language>
    <projectUrl>https://boosted.orange.com</projectUrl>
    <icon>boosted.png</icon>
    <license type="file">LICENSE.txt</license>
    <copyright>Copyright 2017-2020</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <repository type="git" url="https://github.com/Orange-OpenSource/Orange-Boosted-Bootstrap.git" branch="v4-dev" commit="1d6a593107c8dce82ee997bf6c573941328a0d6d" />
    <dependencies>
      <dependency id="jQuery" version="[3.0.0,4)" />
      <dependency id="popper.js" version="[1.16.1,2)" />
      <dependency id="Tablesorter.Mottie" version="[2.31.3,3)" />
    </dependencies>
    <tags>css mobile-first responsive front-end framework web</tags>
  </metadata>
  <files>
    <file src="LICENSE.txt" target="" />
    <file src="nuget\boosted.png" target="" />
    <file src="dist\css\*.*" target="content\Content" />
    <file src="dist\js\boosted*.js" target="content\Scripts" />
    <file src="dist\js\boosted*.js.map" target="content\Scripts" />

    <file src="dist\css\*.*" target="contentFiles\any\any\wwwroot\css" />
    <file src="dist\js\boosted*.js" target="contentFiles\any\any\wwwroot\js" />
    <file src="dist\js\boosted*.js.map" target="contentFiles\any\any\wwwroot\js" />
  </files>
</package>
